				<xs:element name="HernieuwbareEnergie" type="n:Maandwaarden">
					<xs:annotation>
						<xs:documentation>Hernieuwbaar energiegebruik EPrenTot kWh formule 5.28</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HernieuwbareEnergieVerwarming" type="n:Maandwaarden">
					<xs:annotation>
						<xs:documentation>Hernieuwbare energie Verwarming EPrenH kWh formule 5.30</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HernieuwbareEnergieKoeling" type="n:Maandwaarden">
					<xs:annotation>
						<xs:documentation>Hernieuwbare energie Koeling EPrenC kWh formule 5.34</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HernieuwbaarEnergieTapwater" type="n:Maandwaarden">
					<xs:annotation>
						<xs:documentation>Hernieuwbaare energie Tapwater EPrenW kWh formule 5.35</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="HernieuwbaarOpgewekteElektriciteit" type="n:Maandwaarden">
