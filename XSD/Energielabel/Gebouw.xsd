										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Zomernachtventilatie">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ZomernachtventilatieAanwezig" type="xs:boolean">
