								<xs:complexType>
									<xs:sequence>
										<xs:element name="BinnenThermischeZone" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Staat de LBK binnen de thermische zone</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="VerwarmingsbatterijAanwezig" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Is er een verwarmingsbatterij in luchtbehandelingskast?</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="KoelbatterijAanwezig" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Is er een koelbatterij in luchtbehandelingskast?</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
