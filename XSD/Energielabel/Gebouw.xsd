						<xs:documentation>Hernieuwbaar Opgwekte Electriciteit EPren;el kWh formule 5.39</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RisicoTeHogeTemperaturen" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Rekenzones">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Rekenzone" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Orientatie" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="TOjuliorzi" type="xs:double">
																	<!--
								  <xs:element name="Risico" type = "n:RisicoEnumeration"/>
								  -->
																	<xs:annotation>
																		<xs:documentation> overhitting TOjul;or;zi K formule 5.40</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="or" type="n:OrientatieEnumeration" use="required" />
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PrimaireEnergiePerEnergieDrager" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="AfgenomenPrimaireEnergiePerEnergiedrager" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AfgenomenPrimaireEnergiePrimaireFossiele" type="n:Maandwaarden">
											<xs:annotation>
												<xs:documentation>Afgenomen primaire energie kWh formule EP;del;ci 5.11</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="GebruiksFuncties" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="GebruiksFunctie" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="EnergieGebruikVerwarming" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Energiegebruik Verwarming kWh EH;ci H9</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="EnergieGebruikBevochtiging" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Energiegebruik Bevochtiging kWh Ehum;ci H12</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="EnergieGebruikVentilatie" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Energiegebruik Ventilatie kWh EV;el H11</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="EnergieGebruikVerlichting" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Energiegebruik Verlichting kWh El;el H14</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="EnergieGebruikKoeling" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Energiegebruik Koeling kWh EC;ci H10</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="EnergieGebruikTapwater" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Energiegebruik Tapwater kWh EW;ci H13</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<!-- alle waarden hulpenergie zijn alleen elektrisch -->
																<xs:element name="Hulpenergie" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Hulpenergie kWh Waux;formule tot 5.21</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HulpenergieVerwarming" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Hulpenergie kWh WH;aux</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HulpenergieKoeling" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Hulpenergie kWh WC;aux</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="HulpenergieTapwater" type="n:Maandwaarden">
																	<xs:annotation>
																		<xs:documentation>Hulpenergie kWh WW;aux</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="CO2" type="xs:double"/>
												</xs:sequence>
												<xs:attribute name="type" type="n:EnergiedragerEnumeration" use="required" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="VermedenPrimaireFossieleEnergieWarmteKoude" type="n:Maandwaarden">
								<xs:annotation>
									<xs:documentation>Vermeden Primaire Fossiele Energie geexporteerde warmte en of koude gesommeerd over de voorzieningen kWh Ep;exp;T;gi formule 5.12</xs:documentation>
									<!--                    of moeten we dit per voorziening laten zien, geldt ook voor 5.13 en 5.14</xs:documentation> -->
								</xs:annotation>
							</xs:element>
							<xs:element name="VermedenPrimaireFossieleEnergieElektriciteit" type="n:Maandwaarden">
								<xs:annotation>
									<xs:documentation>Vermeden Primaire Fossiele Energie geexporteerde elektriciteit gesommeerd over de voorzieningen kWh Ep;exp;el;gi formule 5.13</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="VermedenPrimaireFossieleEnergieElektriciteitEigenGebruik" type="n:Maandwaarden">
								<xs:annotation>
									<xs:documentation>Vermeden Primaire Fossiele Energie elektriciteit voor eigengebruik gesommeerd over de voorzieningen kWh Ep;nEPus;el formule 5.14</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SpecifiekElectriciteitsgebruikPerm2" type="xs:double">
								<xs:annotation>
									<xs:documentation>Specifiek Elektriciteitsgebruik per m2 Ag kWh/m2  eEPdel;els;el formule 5.17b</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SpecifiekEnergiegebruikWarmteKoudeOverigPerm2" type="xs:double">
								<xs:annotation>
									<xs:documentation>Specifiek Energiegebruik warmte en of koudelevering per m2 Ag GJ/m2  eEPdel;dh/dw/dc formule 5.18b</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="SpecifiekEnergiegebruikOverigPerm2" type="xs:double">
								<xs:annotation>
									<xs:documentation>Specifiek Energiegebruik overige energiedraggers per m2 Ag aeq/m2  eEPdel;aeq formule 5.19b</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

