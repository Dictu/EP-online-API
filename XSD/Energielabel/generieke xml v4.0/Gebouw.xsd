								</xs:element>
								<xs:element name="Paneel">
									<xs:complexType>
										<xs:sequence>
											<xs:element ref="n:U"/>
											<xs:element ref="n:Kozijnmateriaal" minOccurs="0"/>
											<xs:element ref="n:Isolatiedikte" minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="Dicht">
									<xs:annotation>
										<xs:documentation>Gesloten constructiedeel</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element ref="n:Rc"/>
											<xs:element ref="n:Isolatiedikte" minOccurs="0"/>
											<xs:element name="DikteRietpakket" type="xs:double" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Dikte van het rietpakket [mm]. Weglaten indien geen rietpakket.</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="Vloer">
									<xs:complexType>
										<xs:sequence>
											<xs:element ref="n:Rc"/>
											<xs:element name="Perimeter" type="xs:double">
												<xs:annotation>
													<xs:documentation>Perimeter van de vloer [m]</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="OnderMaaiveld" type="xs:boolean"/>
											<xs:element ref="n:Isolatiedikte" minOccurs="0"/>
											<xs:element name="ReflecterendeFolie" type="xs:boolean" minOccurs="0"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:choice>
							<xs:element ref="n:Belemmeringen" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="id" type="xs:unsignedInt" use="optional"/>
						<xs:attributeGroup ref="n:inklap"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="U" type="xs:double">
		<xs:annotation>
			<xs:documentation>U-waarde (warmteoverdracht) van het constructiedeel [W/(m²·K)]</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="Rc" type="xs:double">
		<xs:annotation>
			<xs:documentation>Rc-waarde (warmteweerstand) van het constructiedeel [m²·K/W]</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="IsolatieAanwezig" type="xs:boolean"/>

	<xs:element name="Isolatiedikte" type="xs:nonNegativeInteger">
		<xs:annotation>
			<xs:documentation>Isolatiedikte van het constructiedeel [mm]. (Alleen) weglaten wanneer geen sprake is van isolatie.</xs:documentation>
		</xs:annotation>
	</xs:element>

	<xs:element name="Kozijnmateriaal" type="n:KozijnmateriaalEnum">
		<xs:annotation>
			<xs:documentation>Materiaal van het kozijn. (Alleen) weglaten indien niet bekend.</xs:documentation>
		</xs:annotation>
	</xs:element>

