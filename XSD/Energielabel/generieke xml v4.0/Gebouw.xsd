							<xs:element name="PVTafgedekt" type="xs:boolean"/>
						</xs:sequence>

						<xs:attribute name="id" type="xs:nonNegativeInteger" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

