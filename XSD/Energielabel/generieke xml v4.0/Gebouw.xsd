								</xs:annotation>
							</xs:element>
							<xs:element name="LineaireWarmtedoorgangscoefficient" type="xs:double">
								<xs:annotation>
									<xs:documentation>ψ [W/m·K]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<!-- TODO: 25%? -->
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="OnverwarmdeRuimten">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="AORuimte" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="n:Constructiedelen"/>
							<xs:element ref="n:Koudebruggen"/>
						</xs:sequence>
						<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" />
					</xs:complexType>
				</xs:element>
				<xs:element name="AOSerre" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="n:Constructiedelen"/>
							<xs:element ref="n:Koudebruggen"/>
							<xs:element name="Balkonafdichting" type="xs:boolean" minOccurs="0"/>
						</xs:sequence>
						<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

