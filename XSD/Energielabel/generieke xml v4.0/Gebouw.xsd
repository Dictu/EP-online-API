					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="Ventilatiesystemen">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Ventilatiesysteem" minOccurs="1" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="n:Omschrijving"/>
							<xs:element name="AangeslotenOppervlak" type="xs:double">
								<xs:annotation>
									<xs:documentation>Het totale oppervlak waarop het ventilatiesysteem is aangesloten.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="VentilatieHoofdtype" type="n:VentilatieHoofdtypeEnumeration">
								<xs:annotation>
									<xs:documentation>Hoofdtype ventilatiesysteem</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="VentilatieSubtype" type="n:VentilatieSubtypeEnumeration">
								<xs:annotation>
									<xs:documentation>Subtype ventilatiesysteem</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="TypeDebietregeling" type="n:TypeDebietregelingEnumeration" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Debietregeling voor ventilatietypen B t/m E</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="WTWaanwezig" type="xs:boolean">
								<!-- lijkt me overbodig omdat er ook een type WTW "geen"is-->
								<xs:annotation>
									<xs:documentation>Of er een WTW aanwezig is.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="WTW">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="TypeWTW" type ="n:TypeWTWEnumeration">
											<xs:annotation>
												<xs:documentation>Type WTW van de LBK</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ConstantVolumeregeling" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Is er sprake van een constant-volumeregeling</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="WTWBypass" type="xs:double" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Bypass percentage van de WTW, in veelvouden van 10%</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="RendementWTW" type="xs:double" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Rendement van de WTW-installatie</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="InpandigeKanalenGeisoleerd" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Zijn de kanalen tussen buiten en de WTW geïsoleerd</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="LBKaanwezig" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Is er een luchtbehandelingskast aanwezig</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Luchtbehandelingskast" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="BinnenThermischeZone" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Staat de LBK binnen de thermische zone</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Luchtdichtheidsklasse" type="n:LuchtdichtheidsklasseEnumeration" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Luchtdichtheidsklasse bij mechanische ventilatie</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="KanalenBuitenThermischeZone" type="xs:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Lopen de ventilatiekanalen buiten de thermische zone</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name ="WisselstroomVentilatorAanwezig" type ="xs:boolean">
								<xs:annotation>
									<xs:documentation>True : als er in het ventilatiesysteem een of meerdere wisselstroomventilatoren staan, False :bij gelijkstroom/onbekend (bij werkelijke ventilatorvermogens /geen ventilator).</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Ventilatoren">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Ventilator" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="n:Omschrijving"/>
													<xs:element name="Gelijkstroom" type="xs:boolean">
														<xs:annotation>
															<xs:documentation>Is het een gelijkstroomventilator</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:choice>
														<xs:sequence>
															<xs:element name="Installatiejaar" type="n:VentilatorInstallatiejaarEnumeration">
																<xs:annotation>
																	<xs:documentation>Installatiejaar van de ventilator</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
														<xs:sequence>
															<xs:element name="Pnom" type="xs:double">
																<xs:annotation>
																	<xs:documentation>Nominaal vermogen van de ventilator</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
														<xs:sequence>
															<xs:element name="Pas" type="xs:double">
																<xs:annotation>
																	<xs:documentation>Asvermogen van de ventilator</xs:documentation>
																</xs:annotation>
															</xs:element>
														</xs:sequence>
													</xs:choice>
												</xs:sequence>
												<xs:attribute name="id" type="xs:nonNegativeInteger" use="optional" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Zomernachtventilatie">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ZomernachtventilatieAanwezig" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Is er zomernachtventilatie aanwezig</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="EnkelzijdigeZomernachtventilatie" type="xs:boolean" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Is er sprake ban enkelzijdige zomernachtventilatie</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="BedieningZomernachtventilatie" type="n:BedieningZomernachtventilatieEnumeration" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Hoe wordt de zomernachtventilatie bediend</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="id" type="xs:nonNegativeInteger" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:element name="ZonneEnergiesystemen">
		<xs:annotation>
			<xs:documentation>Gebouwgebonden productie van elektriciteit §16</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ZonneEnergiesysteem" minOccurs="0" maxOccurs="unbounded">
