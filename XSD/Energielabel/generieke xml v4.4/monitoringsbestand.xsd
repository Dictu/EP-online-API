            <xs:element name="Addition" type="tns:Addition" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Huisnummertoevoeging van het gebouw.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuildingAnnotation" type="tns:BuildingIdentification" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Volgnummer als de combinatie van postcode, huisnummer en -toevoeging niet uniek is.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ProvisionalIdentificationType">
        <xs:sequence>
            <xs:element name="ProjectName" type="tns:NonEmptyString80" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Vrij invoerveld voor bv. de naam van het bouwproject van het gebouw.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProjectObject" type="tns:NonEmptyString80" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Nadere omschrijving van het object waarvoor de berekening is uitgevoerd. Denk aan een perceel- of kavelnummer, of een projectspecifiek woningtype.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProvisionalId" type="tns:ProvisionalIdType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Veld waarin een eerder door EP-online gegenereerd identificatienummer kan worden ingevuld.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="ProvisionalIdType">
        <xs:restriction base="xs:string">
            <xs:length value="32"/>
            <xs:pattern value="[0-9A-F]{32}"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="ObjectInformationType">
        <xs:sequence>
            <xs:element name="BuildingCategory" type="tns:TypeBuilding" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Indeling voor gebouwen, hoofdtype. Is verplicht indien MainBuildingClass = Residential. Niet aanwezig indien MainBuildingClass = Commercial.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BuildingCategorySupplement" type="tns:TypeBuildingSupplement" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Subindeling voor gebouwen, subtype. Verplicht voor hoofdtype 7.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ConstructionYear" type="tns:ConstructionYear" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Bouwjaar van het opnamegebouw.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="YearOfRenovation" type="tns:YearOfRenovation" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Jaar van renovatie van het opnamegebouw.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SurfaceArea" type="tns:SurfaceArea" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Totale gebruiksoppervlakte van de begane grond van het opnamegebouw in m² binnen de thermische schil.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EffectiveSurfaceArea" type="tns:SurfaceArea" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Totale gebruiksoppervlakte van het gebouw².</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BuildingUseType">
        <xs:sequence>
            <xs:element name="UseType" type="tns:Application" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Code van de gebruiksfunctie.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Percentage" type="tns:EpbdPercentage" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation>Percentage van gebruiksfunctie. Bepaling: Σ(Ag gebruiksfunctie) / Σ(Ag totaal).</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:attribute name="scale">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <!--<xs:enumeration value="absolute"></xs:enumeration>-->
                <xs:enumeration value="m2"></xs:enumeration>
            </xs:restriction>
        </xs:simpleType>
    </xs:attribute>

    <!-- EpbdDate: formaat is YYYYMMDD -->
    <xs:simpleType name="EpbdDate">
        <xs:restriction base="xs:string">
            <xs:length value="8"/>
            <xs:pattern value="([0-9]{4}((0[1-9])|(1[0-2]))((0[1-9])|(([1-2][0-9])|(3[0-1]))))"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- EpbdNonEmptyString: Hulptype voor een string die niet leeg mag zijn. -->
    <xs:simpleType name="EpbdNonEmptyString">
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NonEmptyString80">
        <xs:restriction base="tns:EpbdNonEmptyString">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- EpbdNumeric: Algemeen type voor alle numerieke velden.
			Deze hebben allen een precisie van twee decimalen. -->
    <xs:simpleType name="EpbdNumeric">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([0-9]{1,14}(\.[0-9]{2}){0,1}|-1)" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="EpbdNumericOneDecimal">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([0-9]{1,14}(\.[0-9]{1}){0,1}|-1)" />
        </xs:restriction>
    </xs:simpleType>

    <!-- EpbdNumericWithNegative: Algemeen type voor alle numerieke velden.
			  Deze hebben allen een precisie van twee decimalen. -->
    <xs:simpleType name="EpbdNumericWithNegative">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="(-{0,1}[0-9]{1,14}(\.[0-9]{2}){0,1}|-1)" />
        </xs:restriction>
    </xs:simpleType>

    <!-- EpbdBENGResult: BENG1 = energievraag, moet > 0 zijn. -->
    <xs:simpleType name="EpbdEnergiebehoefteResult">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="[0-9]{1,14}\.[0-9]{2}" />
            <xs:minExclusive value="0.00"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- EpbdBENGResult: BENG2 = energiegebruik, kan < 0 zijn. -->
    <xs:simpleType name="EpbdEnergiegebruikResult">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="-{0,1}[0-9]{1,14}\.[0-9]{2}" />
        </xs:restriction>
    </xs:simpleType>

    <!-- EpbdBENGResult: BENG3 = aandeel hernieuwbaar, moet >= 0 zijn. -->
    <xs:simpleType name="EpbdAandeelHernieuwbaarResult">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="[0-9]{1,14}\.[0-9]{1}" />
        </xs:restriction>
    </xs:simpleType>

    <!-- MinusOne: Gebruikt voor waardes die eigenlijk ingevuld zouden
				   moeten worden, maar niet bestaan in V4. -->
    <xs:simpleType name="MinusOne">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="-1" />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="EpbdNumericNotZero">
        <xs:restriction base="tns:EpbdNumeric">
            <xs:minExclusive value="0.00"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="EpbdPercentage">
        <xs:restriction base="xs:decimal">
            <xs:pattern value="([0-9]{1,3}\.[0-9]{2})" />
            <xs:minInclusive value="0.00" />
            <xs:maxInclusive value="100.00" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BagId">
        <xs:restriction base="xs:long">
            <xs:pattern value="[0-9]{16}" />
        </xs:restriction>
    </xs:simpleType>
    <!-- EpbdYear: formaat is YYYY -->
    <xs:simpleType name="EpbdYear">
        <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]{4})"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="EPVKlasse">
        <xs:restriction base="xs:string">
            <xs:enumeration value="EPV basis"></xs:enumeration>
            <xs:enumeration value="EPV hoogwaardig"></xs:enumeration>
            <xs:enumeration value="Niet EPV ready"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- BuildingStatus: „Energieprestatiebepaling in het kader van ...” -->
    <xs:simpleType name="BuildingStatus">
        <xs:restriction base="xs:string">
            <xs:enumeration value="vergunningsaanvraag/melding WKB"/>
            <xs:enumeration value="oplevering"/>
            <xs:enumeration value="bestaand"/>
        </xs:restriction>
    </xs:simpleType>


    <!-- Scope: verblijfsobject (specific) of een overkoepelend gebouw (compound) -->
    <xs:simpleType name="Scope">
        <xs:restriction base="xs:string">
            <xs:enumeration value="specific"></xs:enumeration>
            <xs:enumeration value="compound"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IdentificationMethodType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="BAG"></xs:enumeration>
            <xs:enumeration value="TPG"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BuildingClass">
        <xs:restriction base="xs:string">
            <xs:enumeration value="residential"></xs:enumeration>
            <xs:enumeration value="commercial"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- BuildingIdentification: Extra informatie om gebouw te identificeren als er meerdere
	 gebouwen op een kavel staan. -->
    <xs:simpleType name="BuildingIdentification">
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Class: Energieklasse. Uitkomst van de certificaatsoftware. -->
    <xs:simpleType name="Class">
        <xs:restriction base="xs:string">
            <xs:enumeration value="A+++++"></xs:enumeration>
            <xs:enumeration value="A++++"></xs:enumeration>
            <xs:enumeration value="A+++"></xs:enumeration>
            <xs:enumeration value="A++"></xs:enumeration>
            <xs:enumeration value="A+"></xs:enumeration>
            <xs:enumeration value="A"></xs:enumeration>
            <xs:enumeration value="B"></xs:enumeration>
            <xs:enumeration value="C"></xs:enumeration>
            <xs:enumeration value="D"></xs:enumeration>
            <xs:enumeration value="E"></xs:enumeration>
            <xs:enumeration value="F"></xs:enumeration>
            <xs:enumeration value="G"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- ConstructionYear: Bouwjaar. -->
    <xs:simpleType name="ConstructionYear">
        <xs:restriction base="tns:EpbdYear" />
    </xs:simpleType>

    <!-- Country: Internationale landcode, de waarde is altijd NL -->
    <xs:simpleType name="Country">
        <xs:restriction base="xs:string">
            <xs:enumeration value="NL"></xs:enumeration>
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- Version: Versienummer van het EPG XML-format. De waarde is altijd 6.00. Oudere interface versies worden niet ondersteund. -->
    <xs:simpleType name="Version">
        <xs:restriction base="xs:string">
            <xs:enumeration value="11.00"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <!-- Letter: 'huisletter' - Een hoofdletter (A – Z) of kleine letter (a – z). -->
    <xs:simpleType name="Letter">
        <xs:restriction base="xs:string">
            <xs:maxLength value="1"/>
            <xs:pattern value="[A-Za-z]"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- Extension: 'huisnummertoevoeging' - Maximaal vier alfanumerieke tekens bestaande uit een combinatie van hoofdletters (A – Z), kleine letters (a – z) en/of cijfers (0 – 9). -->
    <xs:simpleType name="Addition">
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
            <xs:pattern value="[A-Za-z0-9]{0,4}"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- DeviceEfficiency: Maatregelen om de energie efficiency van het gebouw te verbeteren. De Cx maatregelen zijn voor utiliteitsbouw, de Hx maatregelen voor woningbouw. -->
    <xs:simpleType name="Improvement">
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Application: Gebruiksfuncties (zie ook tabel 6.3 van NTA8800) -->
    <xs:simpleType name="Application">
        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>Kantoorfunctie</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Gezondheidszorg; bedgebied (klinisch)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Gezondheidszorg; andere gezondheidszorgfunctie (niet klinisch)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>Bijeenkomstfunctie, overig</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="6">
                <xs:annotation>
                    <xs:documentation>Onderwijsfunctie</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="7">
                <xs:annotation>
                    <xs:documentation>Sportfunctie</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="9">
                <xs:annotation>
                    <xs:documentation>Logiesfunctie in een logiesgebouw</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="10">
                <xs:annotation>
                    <xs:documentation>Winkelfunctie</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="11">
                <xs:annotation>
                    <xs:documentation>Celfunctie</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="13">
                <xs:annotation>
                    <xs:documentation>Bijeenkomstfunctie voor kinderopvang</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- NameSoftware: Naam van het certificaatsoftwarepakket. -->
    <xs:simpleType name="NameSoftware">
        <xs:restriction base="tns:EpbdNonEmptyString">
            <xs:maxLength value="75"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- Number: Huisnummer van de kavel waarop het te certificeren gebouw aanwezig is. -->
    <xs:simpleType name="Number">
        <xs:restriction base="xs:integer" />
    </xs:simpleType>
    <!-- NumberOfBuildings: Aantal woningen. -->
    <xs:simpleType name="NumberOfBuildings">
        <xs:restriction base="xs:integer" />
    </xs:simpleType>
    <!-- SurfaceArea: Totale gebruiksoppervlak (Ag)-->
    <xs:simpleType name="SurfaceArea">
        <xs:union memberTypes="tns:EpbdNumeric" />
    </xs:simpleType>
    <!-- SurveyDate: Opnamedatum van het certificaat. -->
    <xs:simpleType name="SurveyDate">
        <xs:restriction base="tns:EpbdDate" />
    </xs:simpleType>
    <!-- TypeBuilding: Woningtype. -->
    <xs:simpleType name="TypeBuilding">
        <xs:restriction base="xs:integer">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>Vrijstaande woning</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="12">
                <xs:annotation>
                    <xs:documentation>Twee-onder-één-kap</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Rijwoning hoek</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Rijwoning tussen</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="7">
                <xs:annotation>
                    <xs:documentation>Appartement</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="8">
                <xs:annotation>
                    <xs:documentation>Woongebouw met niet-zelfstandige woonruimte</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="13">
                <xs:annotation>
                    <xs:documentation>Woonboot bestaande ligplaats</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="14">
                <xs:annotation>
                    <xs:documentation>Woonwagen</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="15">
                <xs:annotation>
                    <xs:documentation>Woonboot nieuwe ligplaats</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="16">
                <xs:annotation>
                    <xs:documentation>Logieswoning</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!--TypeBuildingSupplement: Woning type aanvullende rubricering.-->
    <xs:simpleType name="TypeBuildingSupplement">
        <xs:restriction base="xs:string">
            <xs:enumeration value="1">
                <xs:annotation>
                    <xs:documentation>Hoek/dak</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2">
                <xs:annotation>
                    <xs:documentation>Hoek/midden</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="3">
                <xs:annotation>
                    <xs:documentation>Hoek/vloer</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>Tussen/dak</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation>Tussen/midden</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="6">
                <xs:annotation>
                    <xs:documentation>Tussen/vloer</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="7">
                <xs:annotation>
                    <xs:documentation>Tussen/dak/vloer</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="8">
                <xs:annotation>
                    <xs:documentation>Hoek/dak/vloer</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <!-- TypeCalculation: Berekeningswijze.-->
    <xs:simpleType name="TypeCalculation">
        <xs:restriction base="tns:EpbdNonEmptyString">
            <xs:maxLength value="35"/>
            <xs:enumeration value="NTA_8800"/>
            <xs:enumeration value="NTA_8800:2022"/>
            <xs:enumeration value="NTA_8800:2023"/>
            <xs:enumeration value="NTA_8800:2024"/>
            <xs:enumeration value="NTA_8800:2025"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- VersionSoftware: Versienummer van het certificaatsoftwarepakket. -->
    <xs:simpleType name="VersionSoftware">
        <xs:restriction base="tns:EpbdNonEmptyString">
            <xs:maxLength value="75"/>
        </xs:restriction>
    </xs:simpleType>
    <!-- YearOfRenovation: Renovatiejaar gebouw. -->
    <xs:simpleType name="YearOfRenovation">
        <xs:restriction base="tns:EpbdYear" />
    </xs:simpleType>
    <!-- ZipCode: Postcode van het te certificeren gebouw. -->
    <xs:simpleType name="ZipCode">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{4}[A-Z]{2}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="RcWaarde">
        <xs:restriction base="tns:EpbdNumeric"/>
    </xs:simpleType>
    <xs:simpleType name="UWaarde">
        <xs:restriction base="tns:EpbdNumeric"/>
    </xs:simpleType>

    <xs:simpleType name="Opnametype">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Opnameprotocol basismethode">
                <xs:annotation>
                    <xs:documentation>Gegevens zijn gebaseerd op de opname van het gebouw volgens het opnameprotocol (basismethode).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Opnameprotocol detailmethode">
                <xs:annotation>
                    <xs:documentation>Gegevens zijn gebaseerd op de opname van het gebouw volgens het opnameprotocol (detailmethode).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>
