<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
		   targetNamespace="http://schemas.ep-online.nl/monitoringbestand"
		   xmlns:tns="http://schemas.ep-online.nl/monitoringbestand"
		   xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
           xmlns:xf="http://www.w3.org/2002/06/xmldsig-filter2"
		   xml:lang="nl">
    <!-- Namespace urn:nl.nen7120.export kunnen we niet meer opnemen omdat het mogelijk moet zijn
		 dat element SurveySourceData verschillende type XML moet kunnen bevatten. -->
    <!--xmlns:survey="urn:nl.nta.8800.export">-->
    <!-- Remove xmlns:survey before generating the C# classes -->

    <!-- Remove import before generating the C# classes -->
    <!--<xs:import schemaLocation="surveydata.xsd" namespace="urn:nl.nta.8800.export" />-->

    <xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="..\xmldsig-core-schema.xsd"/>
    <xs:import namespace="http://www.w3.org/2002/06/xmldsig-filter2" schemaLocation="..\xmldsig-filter2.xsd"/>
    <xs:import namespace="http://uri.etsi.org/01903/v1.3.2#" schemaLocation="..\XAdESv132.xsd"/>
    
    <xs:element name="Energieprestatie" type="tns:Energieprestatie" />

    <xs:complexType name="Energieprestatie">
        <xs:sequence>
            <xs:element name="EPMeta" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Version" type="tns:Version" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Versiecode van de xml-structuur van het monitoringbestand.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Country" type="tns:Country" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Landcode van het monitoringbestand.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="Tooling" minOccurs="1" maxOccurs="1">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="VendorSoftwareKey" type="tns:NameSoftware" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Identificatiesleutel van de rekensoftware. Waarde moet overeenkomen met de sleutel van een geregistreerd rekensoftware-pakket.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="VendorSoftwareVersionId" type="tns:VersionSoftware" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Identificatie van de versie van de rekensoftware.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element ref="ds:Signature" minOccurs="0" maxOccurs="1"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="EPObject" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="MainBuilding" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Blok met gegevens over het opnamegebouw.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="MainBuildingClass" type="tns:BuildingClass" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Waarde die aangeeft of het opnamegebouw onder woningbouw of utiliteitsbouw valt. De waarde “residential” wordt gebruikt voor woningbouw, “commercial” voor utiliteitsbouw.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ObjectLocation" type="tns:ObjectLocationType" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Blok met gegevens over locatie van het opnamegebouw.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ObjectInformation" type="tns:ObjectInformationType" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Blok met aanvullende gegevens over het opnamegebouw.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="MainBuildingUse" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Blok met gebruik van het opnamegebouw. Is verplicht bij MainBuildingClass = Commercial, maar is niet aanwezig bij MainBuildingClass = Residential.</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="PrimaryUse" type="tns:BuildingUseType" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Blok met detailgegevens voor het primaire gebruik van het opnamegebouw.
                                                            Om het primaire gebruik te bepalen, sommer de gebruiksoppervlakten per gebruiksfunctie. De gebruiksfunctie met het totale gesommeerde grootste oppervlak, is de primaire gebruiksfunctie.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="SecondaryUse" type="tns:BuildingUseType" minOccurs="0" maxOccurs="10">
                                                    <xs:annotation>
                                                        <xs:documentation>Blok(ken) met detailgegevens voor secundair, tertiair gebruik etc. van het opnamegebouw.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="NumberOfDwellings" type="tns:NumberOfBuildings" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Het aantal woningen of wooneenheden in het gebouw. Voor utiliteitsbouw geldt hier verplicht de waarde 0, voor een regulier energielabel in de woningbouw geldt hier verplicht de waarde 1, voor een energielabel van een complex van woning geldt hier het aantal woningen of wooneenheden in het complex waar de opname op van toepassing is.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="ReferenceBuildingList" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Blok met lijst gebouwen waarvoor de energielabels op basis van representativiteit zijn afgegeven.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="ReferenceBuilding" minOccurs="1" maxOccurs="unbounded">
                                        <xs:annotation>
                                            <xs:documentation>Blok met gegevens van een enkel gebouw waarvoor het energielabel is afgeven op basis van representativiteit. Dit betreft dus niet het gebouw benoemd onder MainBuilding.</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="ObjectLocation" type="tns:ObjectLocationType" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Blok met gegevens over locatie van het representatieve pand of verblijfsobject. </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="ObjectInformation" type="tns:ObjectInformationType" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Blok met aanvullende gegevens over het representatieve pand of verblijfsobject.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="EPSurvey" minOccurs="1" maxOccurs="1">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Summary" minOccurs="1" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation>Samenvatting van hoofdkenmerken van het energielabel.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="Labelklasse" type="tns:Class" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Energielabelklasse</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorEnergiebehoefte" type="tns:EpbdEnergiebehoefteResult" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Energiebehoefte-indicator EweH+C;nd;ventsys=C1. §5.3.1.1 in kWh/m2</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorPrimaireFossieleEnergie" type="tns:EpbdEnergiegebruikResult" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Primaire fossiele energie-indicator EwePTot. §5.3.1.2 in kWh/m2</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorPrimaireFossieleEnergieEMGForfaitair" type="tns:EpbdEnergiegebruikResult" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>EMG forfaitaire waarde van de indicator ‘primaire fossiele energie’. §5.3.1.2 in kWh/m2</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorAandeelHernieuwbareEnergie" type="tns:EpbdAandeelHernieuwbaarResult" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Aandeel hernieuwbare energie RERPrenTot. §5.3.1.3 in %</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorHernieuwbareEnergie" type="tns:EpbdEnergiegebruikResult" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Absolute hoeveelheid hernieuwbare energie in kWh EPrenTot. §5.6 in kWh</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorAandeelHernieuwbareEnergieEMGForfaitair" type="tns:EpbdAandeelHernieuwbaarResult" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>EMG forfaitaire waarde van de indicator ‘aandeel hernieuwbare energie’.§5.3.1.3 in %</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="IndicatorHernieuwbareEnergieEMGForfaitair" type="tns:EpbdEnergiegebruikResult" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Absolute hoeveelheid hernieuwbare energie in kWh EPrenTot bij forfaitaire EMG. §5.6</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="EisEnergiebehoefte" type="tns:EpbdEnergiebehoefteResult" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Eis voor energiebehoefteindicator in kWh/m2.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="EisPrimaireFossieleEnergie" type="tns:EpbdEnergiegebruikResult" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation></xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="EisAandeelHernieuwbareEnergie" type="tns:EpbdAandeelHernieuwbaarResult" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation></xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="TOjuliNTA8800" type="tns:EpbdNumeric" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>De maximale getalswaarde voor het risico van te hoge temperaturen in de maand juli. §5.7</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="EisTOjuliNTA8800" type="tns:EpbdNumeric" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation></xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="TOjuliGTO" type="xs:integer" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Aantal uren temperatuuroverschrijding per jaar.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="EisTOjuliGTO" type="xs:integer" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Eis voor aantal uren temperatuuroverschrijding per jaar (450).</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="NettoWarmtebehoefte" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Netto warmtebehoefte in kWh/m² formule 5.3a</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="EPVklasse" type="tns:EPVKlasse" minOccurs="0" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Woning EPV ready: voldoet de woning aan EPV Basis, EPV Hoogwaardig, of is de woning niet EPV ready? Bepalen uit compactheid, warmtebehoefte, EP2 en type woning.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="SurveyDate" type="tns:SurveyDate" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Opnamedatum. Notatie YYYYMMDD.</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="NettoKoudebehoefte" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Koudebehoefte van het gebouw EC;nd kWh/(m2.a) formule 5.3d</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="TotaleWarmteKoudeBehoefte" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Totale behoefte van het gebouw E;H+C;nd kWh/(m2.a) formule 5.3g</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="FinaalEnergiegebruik" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Finaal energiegebruik indicator E;we;final (kWh/m2) formule 5.3h</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="FinaalEnergiegebruikEED" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Finaal energiegebruik indicator E;we;final;EED (kWh/m2) formule 5.3i</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="ZEBIndicator" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Primair-totale-energie-indicator E;we;Plzeb;tot kWh/m2 formule AB.1</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="SurveyMethod" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Blok met gegevens over de gebruikte opnamemethodiek.</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Scope" type="tns:Scope" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Specific = verblijfsobject. Compound = pand. Bij woningbouw geldt dat bij scope compound er geen energielabel wordt gegenereerd. Dit wordt gebruikt bij woongebouwen waarvoor berekening tbv BENG-eisen moet worden geregistreerd. Bij utiliteitsbouw kan berekening voor een gebouw (= compound) wel leiden tot een energielabel, voor bedrijfsverzamelgebouwen. De adviseur kan dat aangeven in de software. Indien er in dat geval een labelletter in de xml meekomt, dan genereert EP-Online een pdf.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="TypeCalculation" type="tns:TypeCalculation" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Aanduiding rekenmethodiek met jaartal.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="PermitPreNTA" type="xs:boolean" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Bij aanvraag omgevingsvergunning is de berekening met een oude (niet NTA8800) rekenmethodiek gedaan.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Status" type="tns:BuildingStatus" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Wat de aanleiding is voor het registreren van het gebouw.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="AreaEffect" type="xs:boolean" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>Waarde die aangeeft of bij de berekening van de energieprestaties gebiedsgebonden maatregelen van toepassing zijn (EMG). De waarde “yes” geeft aan dat EMG van toepassing is.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Opnametype" type="tns:Opnametype" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>indicator of het een basis- of detailopname is.</xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="Gebruiksoppervlakte" type="tns:EpbdNumericNotZero" minOccurs="1" maxOccurs="1"/>
                                    <xs:element name="Verliesoppervlakte" type="tns:EpbdNumericNotZero" minOccurs="1" maxOccurs="1"/>
                                    <xs:element name="Standaard" type="tns:EpbdNumeric" minOccurs="1" maxOccurs="1">
                                        <xs:annotation>
                                            <xs:documentation>Standaard voor woningisolatie in kWh/m2 (par 5.3.2) of U Renovatiestandaard (par 5.3.1).</xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element name="Compactheid" type="tns:EpbdNumeric" minOccurs="0" maxOccurs="1"/>
                                    <xs:element name="Labelklassegrenzen">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Labelklassegrens" minOccurs="12" maxOccurs="12">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            De berekende grenswaarden van alle energielabelklassen A+++++ t/m G (omdat die afhankelijk zijn van de verhouding van de verschillende gebruiksfuncties in een gebouw). De grenswaarde is de bovengrens van de klasse.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:simpleContent>
                                                            <xs:extension base="tns:EpbdNumeric">
                                                                <xs:attribute name="klasse" type="tns:Class" use="required"/>
                                                            </xs:extension>
                                                        </xs:simpleContent>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                        <xs:unique name="AlleLabelklassegrenzen">
                                            <xs:selector xpath="Labelklassegrens"/>
                                            <xs:field xpath="@klasse"/>
                                        </xs:unique>
                                    </xs:element>
                                    <xs:element name="PrimairFossiel" minOccurs="1" maxOccurs="1">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Elektriciteit" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair elektriciteitsverbruik in kWh . Formule 5.10 ci = el (inclusief effect van PV en batterij). Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Gas" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair gasverbruik in kWh. Formule 5.10 ci = gas. Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Warmte" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair warmteverbruik in kWh. Formule 5.10 ci = dh+dw. Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="WarmteEMGforfaitair" type="xs:integer" minOccurs="0" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair warmteverbruik in kWh. Formule 5.10 ci = dh+dw (op basis van forfaitair EMG). Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Koude" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair koudeverbruik in kWh. Formule 5.10 ci = dc. Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="KoudeEMGForfaitair" type="xs:integer" minOccurs="0" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair koudeverbruik in kWh. Formule 5.10 ci = dc (op basis van forfaitair EMG). Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Biomassa" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair biomassaverbruik in kWh. Formule 5.10 ci = bm. Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Stookolie" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Primair stookolieverbruik in kWh. Formule 5.10 ci = oil. Alle maanden optellen.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="TotaalkWh" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Totaal primair verbruik Formule 5.9.
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="TotaalkWhEMGForfaitair" type="xs:integer" minOccurs="0" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Totaal primair verbruik, Formule 5.9 (op basis van EMG forfaitair).
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="Finaal" minOccurs="1" maxOccurs="1">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="Elektriciteit" type="xs:integer" minOccurs="1" maxOccurs="1">
                                                    <xs:annotation>
                                                        <xs:documentation>
                                                            Finaal elektriciteitsverbruik in kWh . Formule 5.58 ci = el
                                                        </xs:documentation>
                                                    </xs:annotation>
                                                </xs:element>
                                                <xs:element name="Elektriciteitopbrengst" type="xs:integer" minOccurs="1" maxOccurs="1">
