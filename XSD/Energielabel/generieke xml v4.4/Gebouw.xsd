                                                                        <xs:documentation>Regeling van de ruimtetemperatuur</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="Bevochtiging" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>De bevochtingsgerelateerde informatie voor deze rekenzone. Weglaten bij woningbouw, of wanneer er geen bevochtiging is.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Bevochtigingssysteem" type="n:Reference">
