    <xs:element name="Constructiedelen">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Constructiedeel" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element ref="n:Omschrijving" minOccurs="0"/>
                            <xs:element name="Begrenzing" type="n:BegrenzingEnumeration" />
                            <xs:choice minOccurs="0" maxOccurs="1">
                                <xs:element name="GekoppeldeVloer" type="n:Reference"/>
                                <xs:element name="GrenstAanAOSerre" type="n:Reference"/>
                                <xs:element name="GrenstAanAORuimte" type="n:Reference"/>
                            </xs:choice>
                            <xs:element ref="n:Hellingshoek"/>
                            <xs:element name="Orientatie" type="n:OrientatieOnbekendEnumeration" />
                            <xs:element name="Oppervlakte" type="xs:double">
                                <xs:annotation>
                                    <xs:documentation>
                                        Het gaat om de netto oppervlakte (dus met aftrek van de ramen) en niet de bruto oppervlakte.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Vlaktype" type="n:VlaktypeEnumeration">
                                <xs:annotation>
                                    <xs:documentation>Type dichte constructie.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:choice>
                                <xs:element name="Raam">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element ref="n:U"/>
                                            <xs:element name="g" type="xs:double">
                                                <xs:annotation>
                                                    <xs:documentation>g-waarde (zontoetredingsfactor) van het raam bij loodrechte inval. [-]</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element name="Beglazing" type="n:GlastypeEnum" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Type glas</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element ref="n:Kozijnmateriaal" minOccurs="0"/>
                                            <xs:element name="Zonwering" type="n:ZonweringEnumeration" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Soort zonwering, indien aanwezig; anders weglaten.</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element name="ZonweringAutomatisch" type="xs:boolean" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Of de zonwering automatisch bediend wordt. Weglaten als er geen sprake is van zonwering.</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="Deur">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element ref="n:U"/>
                                            <xs:element ref="n:IsolatieAanwezig" minOccurs="0" />
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="Paneel">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element ref="n:U"/>
                                            <xs:element ref="n:Kozijnmateriaal" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Type kozijn.</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element ref="n:Isolatiedikte" minOccurs="0"/>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="Dicht">
                                    <xs:annotation>
                                        <xs:documentation>Gesloten constructiedeel</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element ref="n:Rc"/>
                                            <xs:element ref="n:Isolatiedikte" minOccurs="0"/>
                                            <xs:element name="DikteRietpakket" type="xs:double" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>Dikte van het rietpakket [mm]. Weglaten indien geen rietpakket.</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="Vloer">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element ref="n:Rc"/>
                                            <xs:element name="Perimeter" type="xs:double">
                                                <xs:annotation>
                                                    <xs:documentation>Perimeter van de vloer [m]</xs:documentation>
                                                </xs:annotation>
                                            </xs:element>
                                            <xs:element name="OnderMaaiveld" type="xs:boolean"/>
                                            <xs:element ref="n:Isolatiedikte" minOccurs="0"/>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:choice>
                            <xs:element ref="n:BelemmeringenConstructiedeel" minOccurs="0"/>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:unsignedInt"/>
                        <xs:attribute name="kwaliteitsverklaring" type="xs:positiveInteger" use="optional"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="U" type="xs:double">
        <xs:annotation>
            <xs:documentation>U-waarde van het constructiedeel [W/(m²·K)]</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="Rc" type="xs:double">
        <xs:annotation>
            <xs:documentation>Rc-waarde van het constructiedeel [m²·K/W]</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="IsolatieAanwezig" type="xs:boolean"/>

    <xs:element name="Isolatiedikte" type="xs:nonNegativeInteger">
        <xs:annotation>
            <xs:documentation>Isolatiedikte van het constructiedeel [mm]. (Alleen) weglaten wanneer geen sprake is van isolatie.</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="Kozijnmateriaal" type="n:KozijnmateriaalEnum">
        <xs:annotation>
            <xs:documentation>Type kozijn.</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="Koudebruggen">
        <xs:annotation>
            <xs:documentation>Blok met gegevens koudebruggen.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
