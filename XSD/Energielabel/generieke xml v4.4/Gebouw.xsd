                </xs:element>
                <xs:element name="PrimaireFossieleEnergieIndicator" type="xs:double">
                    <xs:annotation>
                        <xs:documentation>Primaire Fossiele Energie Indicator EwePTot kWh/(m2.a) formule 5.2</xs:documentation>
                    </xs:annotation>
                </xs:element>
