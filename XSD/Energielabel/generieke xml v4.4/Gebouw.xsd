                            <xs:element name="Orientatie" type="n:OrientatieEnumeration">
                                <xs:annotation>
                                    <xs:documentation>Minimaal de oriëntatie met de hoogste TOjuli waarde, andere orientaties optioneel.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element ref="n:Belemmeringen" minOccurs="0"/>
                            <xs:choice>
                                <xs:group ref="n:Zonnecollector" />
                                <xs:group ref="n:PV-Systeem" />
                            </xs:choice>
